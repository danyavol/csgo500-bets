{
    "manifest_version": 2,

    "name": "Автоставка CSGO500",

    "version": "2.0",
    
    "icons": {
        "128": "icon128.png",
        "16": "icon16.png"
    },

    "content_scripts": [
        {
            "matches": [ "*://csgo500.com/wheel" ],
            "js": [ "js/csgo500.js" ]
        }
    ],

    "background": {
        "scripts": ["js/background.js"]
    },

    "permissions": [
        "storage"
    ],
  
    "browser_action": {
        "default_title": "CSGO666",
        "default_popup": "popup.html"
    }
      
}