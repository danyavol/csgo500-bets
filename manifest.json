{
    "manifest_version": 2,

    "name": "CSGO666",

    "version": "1.0",

    "content_scripts": [
        {
            "matches": [ "*://csgo500.com/wheel" ],
            "js": [ "js/csgo500.js" ]
        }
    ],

    "background": {
        "scripts": ["js/background.js"]
    },

    "permissions": [
        "storage"
    ],
  
    "browser_action": {
        "default_title": "CSGO666",
        "default_popup": "popup.html"
    }
      
}