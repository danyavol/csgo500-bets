{
    "manifest_version": 2,

    "name": "CSGO666",

    "version": "1.0",

    "content_scripts": [
        {
            "matches": [ "*://csgo500.com/*" ],
            "js": [ "js/csgo500.js" ]
        }
    ],

    "background": {
        "scripts": ["js/main.js"]
    },
  
    "browser_action": {
        "default_title": "CSGO666",
        "default_popup": "popup.html"
    }
      
}